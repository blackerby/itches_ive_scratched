{
  "manifest_version": 2,

  "name": "url2markdown",
  "description": "This extension formats the title of the page and its url into Markdown.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "32x20-solid.png",
    "default_popup": "popup.html",
	"default_title": "Get Markdown link"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}